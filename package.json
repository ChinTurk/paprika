{
  "name": "@paprika/paprika",
  "description": "",
  "scripts": {
    "cypress:ci": "run-p --race storybook cypress:run",
    "cypress:dev": "run-p --race storybook cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "docz:build": "cd packages/documentation && docz build",
    "docz:dev": "cd packages/documentation && docz dev",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint packages",
    "package": "./scripts/package/index.js",
    "prepush": "yarn lint && yarn jest && yarn prettier -- --list-different",
    "prettier:format": "yarn prettier -- --write",
    "prettier": "prettier \"**/*.+(js|json|yml|css|scss|md|mdx)\"",
    "storybook:build": "build-storybook -c .storybook -o storybook-dist",
    "storybook": "start-storybook -p 9009 -c .storybook",
    "jest": "jest",
    "tokens": "cd packages/tokens && yarn start"
  },
  "engines": {
    "yarn": ">=1.12.3"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.12",
    "@storybook/addon-knobs": "^4.1.12",
    "@storybook/addon-links": "^4.1.12",
    "@storybook/addons": "^4.1.12",
    "@storybook/react": "^4.1.12",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.5.1",
    "css-loader": "^2.1.0",
    "cypress": "^3.1.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "lerna": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "pa11y": "^5.0.4",
    "prettier": "^1.16.3",
    "react": "16.8",
    "react-axe": "^3.0.1",
    "react-dom": "16.8",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.4",
    "style-loader": "^0.23.1",
    "styled-components": "^3.3.3"
  }
}
